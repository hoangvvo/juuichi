{
  "name": "undecim",
  "type": "module",
  "version": "0.1.1",
  "description": "Elegant APIs for undici",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build:js": "rollup -c",
    "build:dts": "tsc",
    "build": "npm run clean && npm run build:dts && npm run build:js",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "npx prettier ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangvvo/undecim.git"
  },
  "author": "Hoang Vo (https://hoangvvo.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hoangvvo/undecim/issues"
  },
  "homepage": "https://github.com/hoangvvo/undecim#readme",
  "engines": {
    "node": "^12.0.0 || ^14.0.0 || >=16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^17.0.18",
    "rollup": "^2.67.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "undici": "^4.14.1"
  }
}
